<<h1>Users details </h1>

<%= form_for @color,  url: color_index_path ,method: :post  do |t|  %>

	<%= label_tag(:colorname, 'Enter_color_Name ' ) %>
  <%= t.text_field :colorname  %>
	<br>
	<br>
	<br>

	<%= label_tag(:age, 'Enter_The_Age ' ) %>
  <%= t.text_field :age  %>
	<br>
	<br>
	<br>
  
	<%= submit_tag('submit')  %>

<% end %>


